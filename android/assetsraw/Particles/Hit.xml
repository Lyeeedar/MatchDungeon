<Effect xmlns:meta="Editor">
	<Loop>false</Loop>
	<Emitters>
		<Emitter>
			<Name>Flash</Name>
			<Width>1</Width>
			<Height>1</Height>
			<SizeMultiplier>
				<Keyframe>0|1</Keyframe>
			</SizeMultiplier>
			<ParticleSpeed>0,0</ParticleSpeed>
			<ParticleRotation>0,360</ParticleRotation>
			<RateKeyframes>
				<Keyframe>0|1</Keyframe>
			</RateKeyframes>
			<SingleBurst>true</SingleBurst>
			<Particles>
				<Particle>
					<Lifetime>0.1,0.1</Lifetime>
					<AllowResize>false</AllowResize>
					<SizeKeyframes>
						<Stream>
							<Keyframe>0|0.8|0.8</Keyframe>
						</Stream>
					</SizeKeyframes>
					<BlendMode>Additive</BlendMode>
					<BlendKeyframes>true</BlendKeyframes>
					<TextureKeyframes>
						<Stream>
							<Keyframe>0|EffectSprites/Hit/Hit_1</Keyframe>
							<Keyframe>0.3|EffectSprites/Hit/Hit_2</Keyframe>
							<Keyframe>0.6|EffectSprites/Hit/Hit_3</Keyframe>
							<Keyframe>0.9|EffectSprites/Hit/Hit_4</Keyframe>
						</Stream>
					</TextureKeyframes>
					<ColourKeyframes>
						<Stream>
							<Keyframe>0|255,255,255,255</Keyframe>
						</Stream>
					</ColourKeyframes>
					<AlphaKeyframes>
						<Stream>
							<Keyframe>0|1</Keyframe>
							<Keyframe>0.7|1</Keyframe>
							<Keyframe>1|0</Keyframe>
						</Stream>
					</AlphaKeyframes>
					<AlphaRefKeyframes>
						<Stream>
							<Keyframe>0|0</Keyframe>
						</Stream>
					</AlphaRefKeyframes>
				</Particle>
			</Particles>
		</Emitter>
	</Emitters>
</Effect>