<Effect xmlns:meta="Editor">
	<Loop>false</Loop>
	<Emitters>
		<Emitter>
			<Name>Hearts</Name>
			<Shape>Cone</Shape>
			<Angle>90</Angle>
			<Width>90</Width>
			<Height>1</Height>
			<SizeMultiplier>
				<Keyframe>0|1</Keyframe>
			</SizeMultiplier>
			<ParticleSpeed>0,1</ParticleSpeed>
			<ParticleRotation>-30,30</ParticleRotation>
			<RateKeyframes>
				<Keyframe>0|20</Keyframe>
			</RateKeyframes>
			<SingleBurst>true</SingleBurst>
			<Particles>
				<Particle>
					<Lifetime>0.5,0.7</Lifetime>
					<VelocityAligned>true</VelocityAligned>
					<SizeKeyframes>
						<Stream>
							<Keyframe>0|0.2|0.3</Keyframe>
						</Stream>
					</SizeKeyframes>
					<BlendMode>Additive</BlendMode>
					<TextureKeyframes>
						<Stream>
							<Keyframe>0|Particle/heart</Keyframe>
						</Stream>
					</TextureKeyframes>
					<ColourKeyframes>
						<Stream>
							<Keyframe>0|255,255,255,255</Keyframe>
						</Stream>
					</ColourKeyframes>
					<AlphaKeyframes>
						<Stream>
							<Keyframe>0|1</Keyframe>
							<Keyframe>0.9|1</Keyframe>
							<Keyframe>1|0</Keyframe>
						</Stream>
					</AlphaKeyframes>
					<AlphaRefKeyframes>
						<Stream>
							<Keyframe>0|0</Keyframe>
						</Stream>
					</AlphaRefKeyframes>
				</Particle>
			</Particles>
		</Emitter>
	</Emitters>
</Effect>