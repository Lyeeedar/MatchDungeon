<Effect xmlns:meta="Editor">
	<Loop>false</Loop>
	<Emitters>
		<Emitter>
			<Name>Shockwave</Name>
			<Width>0</Width>
			<Height>0</Height>
			<ParticleSpeed>0,0</ParticleSpeed>
			<ParticleRotation>0,360</ParticleRotation>
			<RateKeyframes>
				<Keyframe>0|1</Keyframe>
			</RateKeyframes>
			<SingleBurst>true</SingleBurst>
			<Particles>
				<Particle>
					<Lifetime>0.2,0.2</Lifetime>
					<BlendMode>Additive</BlendMode>
					<TextureKeyframes>
						<Stream>
							<Keyframe>0|Particle/ring</Keyframe>
						</Stream>
					</TextureKeyframes>
					<SizeKeyframes>
						<Stream>
							<Keyframe>0|0|0</Keyframe>
							<Keyframe>1|1.5|1.5</Keyframe>
						</Stream>
					</SizeKeyframes>
					<ColourKeyframes>
						<Stream>
							<Keyframe>0|255,255,255,255</Keyframe>
						</Stream>
					</ColourKeyframes>
					<AlphaKeyframes>
						<Stream>
							<Keyframe>0|0.6</Keyframe>
							<Keyframe>0.8|0.6</Keyframe>
							<Keyframe>1|0</Keyframe>
						</Stream>
					</AlphaKeyframes>
				</Particle>
			</Particles>
		</Emitter>
		<Emitter>
			<Name>Lines</Name>
			<Shape>Circle</Shape>
			<Width>0.1</Width>
			<Height>0.1</Height>
			<Area>Border</Area>
			<ParticleSpeed>3,5</ParticleSpeed>
			<ParticleRotation>0,360</ParticleRotation>
			<RateKeyframes>
				<Keyframe>0|10</Keyframe>
			</RateKeyframes>
			<SingleBurst>true</SingleBurst>
			<Particles>
				<Particle>
					<Lifetime>0.3,0.4</Lifetime>
					<BlendMode>Additive</BlendMode>
					<VelocityAligned>true</VelocityAligned>
					<TextureKeyframes>
						<Stream>
							<Keyframe>0|Particle/line</Keyframe>
						</Stream>
					</TextureKeyframes>
					<SizeKeyframes>
						<Stream>
							<Keyframe>0|0.2|0.2</Keyframe>
						</Stream>
					</SizeKeyframes>
					<ColourKeyframes>
						<Stream>
							<Keyframe>0|255,255,255,255</Keyframe>
						</Stream>
					</ColourKeyframes>
					<AlphaKeyframes>
						<Stream>
							<Keyframe>0|0</Keyframe>
							<Keyframe>0.1|0.7</Keyframe>
							<Keyframe>0.6|0.6</Keyframe>
							<Keyframe>1|0</Keyframe>
						</Stream>
					</AlphaKeyframes>
				</Particle>
			</Particles>
		</Emitter>
	</Emitters>
</Effect>