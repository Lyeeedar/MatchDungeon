<Effect xmlns:meta="Editor">
	<Loop>false</Loop>
	<Emitters>
		<Emitter>
			<Name>Clouds</Name>
			<Shape>Circle</Shape>
			<Width>0.1</Width>
			<Height>0.1</Height>
			<SizeMultiplier>
				<Keyframe>0|1</Keyframe>
			</SizeMultiplier>
			<Direction>Random</Direction>
			<ParticleSpeed>0.3,0.8</ParticleSpeed>
			<ParticleRotation>0,360</ParticleRotation>
			<RateKeyframes>
				<Keyframe>0|30</Keyframe>
			</RateKeyframes>
			<SingleBurst>true</SingleBurst>
			<Particles>
				<Particle>
					<Lifetime>0.7,1.3</Lifetime>
					<Drag>1</Drag>
					<Brownian>1</Brownian>
					<SizeKeyframes>
						<Stream>
							<Keyframe>0|0|0</Keyframe>
							<Keyframe>1|0.15|0.3</Keyframe>
						</Stream>
					</SizeKeyframes>
					<BlendMode>Multiplicative</BlendMode>
					<TextureKeyframes>
						<Stream>
							<Keyframe>0|Particle/plasma_1</Keyframe>
						</Stream>
						<Stream>
							<Keyframe>0|Particle/plasma_2</Keyframe>
						</Stream>
						<Stream>
							<Keyframe>0|Particle/plasma_3</Keyframe>
						</Stream>
					</TextureKeyframes>
					<ColourKeyframes>
						<Stream>
							<Keyframe>0|114,145,39,255</Keyframe>
						</Stream>
						<Stream>
							<Keyframe>0|105,92,37,255</Keyframe>
						</Stream>
						<Stream>
							<Keyframe>0|106,194,64,255</Keyframe>
						</Stream>
						<Stream>
							<Keyframe>0|153,142,24,255</Keyframe>
						</Stream>
					</ColourKeyframes>
					<AlphaKeyframes>
						<Stream>
							<Keyframe>0|1</Keyframe>
							<Keyframe>0.7251712|1</Keyframe>
							<Keyframe>1|0</Keyframe>
						</Stream>
					</AlphaKeyframes>
					<AlphaRefKeyframes>
						<Stream>
							<Keyframe>0|0</Keyframe>
						</Stream>
					</AlphaRefKeyframes>
				</Particle>
			</Particles>
		</Emitter>
	</Emitters>
</Effect>