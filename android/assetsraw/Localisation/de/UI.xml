	<Text ID="matchmessage.2" TranslateType="Auto" TranslatedHash="772095037">Tolle</Text>
	<Text ID="matchmessage.3" TranslateType="Auto" TranslatedHash="1960734051">spektakulär</Text>
	<Text ID="matchmessage.4" TranslateType="Auto" TranslatedHash="-1801283976">Magisch</Text>
	<Text ID="matchmessage.5" TranslateType="Auto" TranslatedHash="-77594853">legendär</Text>
